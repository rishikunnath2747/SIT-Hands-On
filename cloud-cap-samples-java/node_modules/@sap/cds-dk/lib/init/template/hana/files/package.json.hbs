{
  {{#isNodejs}}
  "dependencies": {
    "@cap-js/hana": "^1"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    }{{#profile}},
    "requires": {
      "[{{profile}}]": {
      {{#shortcut}}
        "db": "hana"
      {{/shortcut}}
      {{^shortcut}}
        "db": {
          "kind": "hana"
        }
      {{/shortcut}}
      }
    }{{/profile}}
  }
  {{/isNodejs}}
  {{#isJava}}
  "requires": {
    {{#profile}}
    "[{{profile}}]": {
    {{/profile}}
    {{#shortcut}}
      "db": "hana"
    {{/shortcut}}
    {{^shortcut}}
      "db": {
        "kind": "hana"
      }
    {{/shortcut}}
    {{#profile}}
    }
    {{/profile}}
  }
  {{/isJava}}
}
